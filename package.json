{
  "name": "@josee9988/minifyall",
  "version": "1.0.0",
  "description": "A CLI tool to minify JSON/C, HTML and CSS.",
  "main": "dist/index.js",
  "bin": {
    "@josee9988/minifyall": "bin/minifyall.js",
    "minifyall": "bin/minifyall.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublish": "rm -R dist/ && npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "build": "tsc",
    "watch": "tsc -watch",
    "lint": "tslint -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest",
    "posttest": "rm tests/.exampleTestFiles/testFile.min.css",
    "removeExtraExampleTestFiles": "cd tests/.exampleTestFiles && rm -v !('jsonCFile.jsonc'|'jsonFile.json'|'styles.css'|'myWebpage.html') && cd ../..",
    "refresh": "rm -R dist/ && rm -R node_modules/ && npm i && npm run build",
    "testPack": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josee9988/MinifyAllCli.git"
  },
  "keywords": [
    "css",
    "json",
    "jsonc",
    "html",
    "css3",
    "html5",
    "minify",
    "minifier"
  ],
  "jest": {
    "preset": "ts-jest"
  },
  "author": "@Josee9988",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Josee9988/MinifyAllCli/issues"
  },
  "homepage": "https://github.com/Josee9988/MinifyAllCli#readme",
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "jest": "^25.0.1",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.0.0",
    "esm": "^3.2.25"
  }
}
